{
  "name": "blockchain-for-insurance",
  "version": "2.1.0",
  "description": "A Blockchain sample NodeJS application for the insurance industry.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ammilanov/blockchain-for-insurance"
  },
  "author": {
    "name": "Angel Milanov",
