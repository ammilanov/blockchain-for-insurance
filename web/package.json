{
  "name": "blockchain-insurance",
  "version": "1.0.0",
  "description": "A Blockchain sample NodeJS application for the insurance industry.",
  "repository": {
    "type": "git",
    "url": "https://hub.jazz.net/git/amilanov/blockchain-insurance"
  },
  "author": {
    "name": "Angel Milanov",
    "email": "milanov@de.ibm.com"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  },
  "main": "./www/server.js",
  "scripts": {
    "start": "nodemon --ignore 'src/' --ignore 'views/' --ignore 'locales/' ./www/server.js --exec babel-node --inspect --debug --presets es2015,stage-2",
    "prebuild": "npm install",
    "build": "npm-run-all build:webpack build:server",
    "build:webpack": "cross-env NODE_ENV=production babel-node ./tools/build",
    "build:server": "babel www -d bin",
    "serve": "cross-env NODE_ENV=production node ./bin/server",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "clean": "rimraf -rf ./dist ./bin",
    "test": "babel-node --inspect test.js"
  },
  "dependencies": {
    "babel-polyfill": "6.22.0",
    "body-parser": "1.16.0",
    "cf-deployment-tracker-client": "0.1.3",
    "compression": "1.6.2",
    "cookie-parser": "1.4.3",
    "cookies": "0.6.2",
    "cross-env": "3.2.3",
    "csurf": "1.9.0",
    "dotenv": "4.0.0",
    "express": "4.14.1",
    "express-rate-limit": "2.6.0",
    "express-status-monitor": "0.1.9",
    "fabric-ca-client": "1.0.0-alpha.0",
    "fabric-client": "1.0.0-alpha",
    "helmet": "3.5.0",
    "i18n": "0.8.3",
    "isomorphic-fetch": "2.2.1",
    "json-style-converter": "1.0.2",
    "lodash": "4.17.4",
    "moment": "2.17.1",
    "moment-timezone": "0.5.11",
    "morgan": "1.7.0",
    "pug": "2.0.0-beta11",
    "react": "15.4.2",
    "react-cookie": "1.0.4",
    "react-dom": "15.4.2",
    "react-intl": "2.2.3",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
