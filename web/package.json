    "json-style-converter": "1.0.3",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "moment-timezone": "0.5.13",
    "morgan": "1.8.2",
    "prop-types": "15.5.10",
    "pug": "2.0.0-rc.2",
    "react": "15.5.4",
    "react-cookie": "2.0.7",
    "react-dom": "15.5.4",
    "react-intl": "2.3.0",
    "react-redux": "5.0.5",
